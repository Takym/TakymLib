{
	"metadata": [
		{
			"src": [
				{
					"files": [ "**/*.csproj" ],
					"src": "../"
				}
			],
			"dest": "api",
			"disableGitFeatures": false,
			"disableDefaultFilter": false,
			"properties": {
				"TargetFrameworks": "net5.0"
			}
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**/*.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"toc.yml",
					"**/*.md"
				]
			}
		],
		"resource": [
			{
				"files": [ "**/images/**" ],
				"exclude": [ "**/bin/**", "**/obj/**", "**.meta" ]
			}
		],
		"xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
		"dest": "_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"statictoc",
			"packages/memberpage/2.56.6/content",
			"template"
		],
		"postProcessors": [],
		"markdownEngineName": "markdig",
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false,
		"disableGitFeatures": false
	}
}
